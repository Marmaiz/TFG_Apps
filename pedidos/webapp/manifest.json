{
  "_version": "1.81.0",
  "sap.app": {
    "id": "pedidos",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.1",
      "toolsId": "e9ff6e00-37c1-4cae-8e5d-c3fed101108f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/pedidos/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "pedidos-display": {
          "semanticObject": "pedidos",
          "action": "display",
          "title": "{{pedidos-display.flpTitle}}",
          "subTitle": "{{pedidos-display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "pedidos.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "pedidos.view",
        "async": true,
        "viewPath": "pedidos.view"
      },
          "routes": [
        {
          "name": "RoutePedidos",
          "pattern": ":?query:",
          "target": [
            "TargetPedidos"
          ]
        },
        {
          "name": "DetallePedido",
          "pattern": "DetallePedido/{Id}",
          "target": [
            "DetallePedido"
          ]
        },
        {
          "name": "ListadoPedidosPorProcesar",
          "pattern": "ListadoPedidosPorProcesar",
          "target": [
            "ListadoPedidosPorProcesar"
          ]
        },
        {
          "name": "ProcesarPedido",
          "pattern": "ProcesarPedido/{Id}",
          "target": [
            "ProcesarPedido"
          ]
        }
      ],
      "targets": {
        "TargetPedidos": {
          "id": "Pedidos",
          "name": "Pedidos"
        },
         "DetallePedido": {
          "name": "DetallePedido"
        },
         "ProcesarPedido": {
          "name": "ProcesarPedido"
        },
          "ListadoPedidosPorProcesar": {
          "name": "ListadoPedidosPorProcesar"
        }
      }
    },
    "rootView": {
      "viewName": "pedidos.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "pedidos"
  }
}
